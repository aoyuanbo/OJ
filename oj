#include <stdio.h>
int main()
{
    int n,m,i=0,j=0,l,f=0,s=0;

    printf("请输入m和n的值.\n");
    do
    {
        scanf("%d %d",&m,&n);
        if(m>10||n>10)
            printf("mn can");
        else break;
    }
    while(1);

     int a[2*m][n];
    printf("input");
    for(i=0; i<(2*m); i++)
        for(j=0; j<n; j++)
        {
            scanf("%d",&l);
            a[i][j]=l;
        }

 i=0,j=0;

 //printf("%d %d %d %d %d %d\n",a[0][0],a[0][1],a[1][0],a[1][1],i,j);
  while(i<m)
      {  while(j<n)
            {if((a[i][j]+a[i+m][j])==0)
                    f++;
                    j++;}
        if(f==n)
        s++;
        f=0;
        i++;
}
    printf("%d",s);
    return 0;
}
